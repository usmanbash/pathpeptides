            <!-- Links -->
            <div class="links md:flex gap-x-8 text-[#ffffff] items-center">
              {%- for link in section.settings.menu.links -%}
                {%- if link.links != blank -%}
              <!-- Dropdown -->
              <div class="relative dropdown-menu group m-0 nav-item">
                <button class="flex items-center uppercase font-light space-x-1 text-white focus:outline-none">
                      <span>{{ link.title }}</span>
                  <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7" />
                  </svg>
                </button>
                <!-- Dropdown Menu -->
                <div class="absolute left-0 mt-2 w-[250px] gap-6  pt-4 pb-4 bg-white border rounded-b-[20px] shadow-lg opacity-0 invisible group-hover:opacity-100 group-hover:visible transition-all duration-200">
                      {%- for child_link in link.links -%}
                        <a href="{{ child_link.url }}" class="flex items-center gap-3 px-4 py-2 text-gray-700 hover:bg-gray-100">
                          {%- capture icon_snippet_name -%}icon-{{ child_link.title | handleize }}{%- endcapture -%}
                           {% if icon_snippet_name %}
                             {% include icon_snippet_name %}
                           {%endif%}
                          {{ child_link.title }}
                        </a>
                      {%- endfor -%}
                </div>
              </div>
                {%- else -%}
                  <a href="{{ link.url }}" class="text-white {% if link.handle != 'faqs' %}uppercase {% endif %}font-light nav-item">{{ link.title }}</a>
