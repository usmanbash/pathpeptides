                  <!-- Notification Bubble -->
                  <span class="absolute -top-1 -right-2 flex items-center justify-center w-5 h-5 rounded-full bg-white text-black text-[9px] font-bold"> 9+ </span>
                </div>
              </div>
            </div>
            <!-- Mobile menu button -->
            <div class="md:hidden">
              <!-- Mobile Hamburger -->
              <button id="mobile-menu-button" class="md:hidden text-white focus:outline-none focus:ring-2 focus:ring-gray-600 rounded-lg p-2">
                <svg id="hamburger-icon" class="w-6 h-6 block" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
                </svg>
                <svg id="close-icon" class="w-6 h-6 hidden" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
                </svg>
              </button>
            </div>
          </div>
        </div>
        <!-- Mobile Menu -->
        <div id="mobile-menu" class="md:hidden absolute top-[72px] left-0 w-full bg-white text-black  rounded-b-[20px] shadow-lg opacity-0 invisible translate-y-[-10px] transition-all duration-300 ease-out">
          <div class="flex flex-col px-6 py-4 space-y-4 text-[20px]">
            {%- for link in section.settings.menu.links -%}
              {%- if link.links != blank -%}
            <!-- Shop with submenu -->
            <div class="m-0">
              <button id="shop-toggle" class="flex justify-between items-center w-full hover:text-gray-500 transition focus:outline-none">
                    <span>{{ link.title }}</span>
                <svg id="shop-chevron" class="w-4 h-4 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 9l6 6 6-6" />
                </svg>
              </button>
              <div id="shop-submenu" class="pl-0 flex flex-col gap-4 space-y-0 overflow-hidden max-h-0 transition-all duration-300 ease-in-out">
                    {%- for child_link in link.links -%}
                      <a href="{{ child_link.url }}" class="flex items-center gap-3 px-0 py-0 text-gray-700 hover:bg-gray-100 m-0 mt-3">
                        {%- capture icon_snippet_name -%}icon-{{ child_link.title | handleize }}{%- endcapture -%}
                        {% include icon_snippet_name %}
                        {{ child_link.title }}
                      </a>
                    {%- endfor -%}
              </div>
            </div>
              {%- else -%}
                <a href="{{ link.url }}" class="hover:text-gray-500 transition">{{ link.title }}</a>
              {%- endif -%}
            {%- endfor -%}
            <!-- Other links -->
            
            <a href="#" class="hover:text-gray-500 transition">Profile</a>
            <a href="#" class="hover:text-gray-500 transition">Cart (9+)</a>
          </div>
        </div>
        <!-- End Mobile Menu -->
      </nav>
    </header> 

